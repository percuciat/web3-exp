{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "camelcase": "off",
    "react/prop-types": 0,
    "no-unused-vars": [
      "warn",
      { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
    ]
  }
}
